	2nd:
	work n+1 chip1
	{576'h0, 64'hb, 32'h1, 32'h1, 32'h1}
	work n+1 chip0
	{576'h0, 64'h6, 32'h1, 32'h1, 32'h1}
	
	3rd:
	work n+2 chip1
	{576'h0, 64'hc, 32'h1, 32'h1, 32'h1}
	work n+2 chip0
	{576'h0, 64'h7, 32'h1, 32'h1, 32'h1}
	
	then get nonce 32'h1beebba0 for chip0, 32'hc1698161 for chip1;

